{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/page.tsx",
    "app/layout.tsx",
    "app/components/SignupCard.tsx",
    "app/components/Navbar.tsx",
    "app/components/BlogSmallCard.tsx",
    "app/components/ArticleCardOne.tsx",
    "app/components/BlogLargeCard.tsx",
    "app/components/RichTextComponent.tsx",
    "app/api/hello/route.js"
  ],
  "exclude": [
    "node_modules"
  ]
}